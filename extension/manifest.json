{
  "name": "Vaibhav's Site Blocker",
  "description": "This extension blocks unwanted sites.",
  "author": "Grzegorz Milka",
  "version": "1.7.8",
  "manifest_version": 2,
  "icons": {
      "128": "g_128.png"
  },
  "permissions": [
    "tabs",
    "webNavigation",
    "webRequest",
    "webRequestBlocking",
    "*://*/*",
    "storage"],
  "background": {
    "scripts": ["background.js"],
    "persistent": true
  },
  "browser_action": {
    "default_icon": "g_128.png",
    "default_popup": "options.html"
  },
  "options_page": "options.html",
  "key": ""
} 